    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Ligas Parana"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActividadPantallaBienvenida"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ActividadLiga"
            android:label="@string/app_name"></activity>
        <activity android:name=".ActividadCategoria"></activity>
        <activity
            android:name=".ActividadPrincipal"
            android:label="Actividad Principal"
